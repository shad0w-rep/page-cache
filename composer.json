{
    "name": "silber/page-cache",
    "description": "Caches responses as static files on disk for lightning fast page loads..",
    "keywords": ["laravel", "cache"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Silber",
            "email": "contact@josephsilber.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Silber\\PageCache\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests"]
    },
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.0 - 5.5",
        "illuminate/filesystem": "5.0 - 5.5",
        "symfony/http-foundation": "^2.6.0|^3.0.0"
    },
    "require-dev": {
        "illuminate/container": "5.0 - 5.5",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^4.8"
    },
    "suggest": {
        "illuminate/console": "Allows clearing the cache via artisan"
    }
}
